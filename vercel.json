{
  "version": 2,
  "builds": [
    {
      "src": "/portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "runtime": "python3.9" ,
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static_cdn"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/portfolio/wsgi.py"
    }
  ]
}